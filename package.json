{
  "name": "@compeydev/musiccomp-discord-bot",
  "version": "0.2.15",
  "description": "MusicComp is a discord music bot with full Spotify, YouTube and Soundcloud API support.",
  "main": "index.js",
  "scripts": {
    "setup": "bash -f ./scripts/log.sh ; bash -f setup.sh > ./logs/install.log 2>&1 ; bash -f ./scripts/success.sh ; yarn start",
    "setup-colorless": "bash -f ./scripts/log.sh -c ; bash -f setup.sh > ./logs/install.log 2>&1 ; bash -f ./scripts/success.sh ; yarn start",
    "start": "node index",
    "rm-logs": "rm Logs.log",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/CompeyDev/MusicComp-v2"
  },
  "engines": {
    "node": ">=12.x"
  },
  "keywords": [
    "discord",
    "discord-bot",
    "discord-music",
    "music",
    "discord-music"
  ],
  "author": "DevComp_",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CompeyDev/MusicComp-v2/issues"
  },
  "homepage": "https://www.devcomp.tk/MusicComp-v2/",
  "dependencies": {
    "colors": "1.4.0",
    "cpu-stat": "^2.0.1",
    "discord.js": "^12.5.3",
    "discordjs-activity": "1.4.1",
    "erela.js": "^2.3.3",
    "erela.js-apple": "^1.2.6",
    "erela.js-deezer": "^1.0.7",
    "erela.js-facebook": "^1.0.4",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "jsoning": "^0.13.23",
    "lavasfy": "2.3.0",
    "lodash": "^4.17.21",
    "lyrics-finder": "^21.7.0",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^6.2.3",
    "musiccomp-discord-bot": "^0.2.14-npm",
    "node-fetch": "^2.6.1",
    "npm-check-updates": "^12.4.0",
    "passport": "^0.5.2",
    "passport-discord": "^0.1.4",
    "pretty-ms": "^7.0.1",
    "socket.io": "^4.4.1",
    "winston": "^3.6.0",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1"
  },
  "lint-staged": {
    "*.{js,json,md}": "prettier --write"
  }
}
